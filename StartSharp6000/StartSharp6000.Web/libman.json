{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "Chart.js@3.7.0",
      "destination": "wwwroot/lib/chartjs",
      "files": [
        "chart.min.js",
        "chart.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "datatables.net@1.11.3",
      "destination": "wwwroot/lib/datatables",
      "files": [
        "js/jquery.dataTables.js",
        "js/jquery.dataTables.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "datatables.net-bs5@1.11.3",
      "destination": "wwwroot/lib/datatables/",
      "files": [
        "css/dataTables.bootstrap5.css",
        "css/dataTables.bootstrap5.min.css",
        "images/favicon.ico",
        "images/sort_asc.png",
        "images/sort_asc_disabled.png",
        "images/sort_both.png",
        "images/sort_desc.png",
        "images/sort_desc_disabled.png",
        "js/dataTables.bootstrap5.js",
        "js/dataTables.bootstrap5.min.js"
      ]
    }
  ]
}